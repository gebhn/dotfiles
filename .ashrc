#!/bin/sh

export PS1='> '
export EDITOR=nvim
export BROWSER=firefox.desktop
export CFLAGS='-O3 -pipe -march=native'
export CXXFLAGS='$CFLAGS'
export MAKEFLAGS='-j$(nproc)'

mksh() {
    printf '#!/bin/sh\n' > "$1" && chmod +x "$1";
}
mkcd() {
    mkdir -p "$@" && cd "$_";
}
oxo() {
    curl -F"file=@$1" https://0x0.st;
}
pb() {
    wl-paste | nc termbin.com 9999;
}
gs() {
    git status;
}
..() {
    cd ../;
}
...() {
    ..; ..;
}
